{
  "name": "vuex-electron",
  "version": "1.0.3",
  "description": "Vuex Electron",
  "keywords": [
    "vue",
    "vuex",
    "electron"
  ],
  "main": "./dist/index.js",
  "repository": "https://github.com/vue-electron/vuex-electron",
  "homepage": "https://github.com/vue-electron/vuex-electron",
  "bugs": "https://github.com/vue-electron/vuex-electron/issues",
  "author": "Andrew Emelianenko <emelianenko.web@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src -d dist --env-name production",
    "watch": "babel src -d dist --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "yarn lint && jest && bundlesize"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "2 kB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-minify": "^0.5.1",
    "bundlesize": "^0.18.0",
    "electron": "^12.2.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "vue": "^2.6.11",
    "vuex": "^3.5.1"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "electron-store": "^8.1.0"
  }
}
